{
    "window.autoDetectColorScheme": true,
    "workbench.colorTheme": "Darcula",
    "workbench.preferredDarkColorTheme": "Darcula",
    "workbench.preferredLightColorTheme": "Night Owl Light (No Italics)",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": false,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindingsNonRecursive": [
      {
        "before": ["<C-a>"],
        "after": ["<C-o>0"]
      },
      {
        "before": ["<C-e>"],
        "after": ["<C-o>$"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      },
      {
        "before": ["<CR>"],
        "commands": [":nohl"]
      },
      {
        "before": ["K"],
        "commands": ["lineBreakInsert"],
        "silent": true
      }
    ],
    "vim.handleKeys": {
      "<C-f>": false
    },
    "vim.leader": "<space>",
    "vim.startInInsertMode": false,
    "editor.lineNumbers": "relative",
    "editor.mouseWheelScrollSensitivity": 0.7,
    "workbench.list.mouseWheelScrollSensitivity": 0.7,
    "terminal.integrated.mouseWheelScrollSensitivity": 0.7,
    "vim.visualstar": true,
    "vim.whichwrap": "",
    "vim.vimrc.enable": true,
    "remote.SSH.defaultExtensions": [
      "ms-toolsai.jupyter",
      "ms-toolsai.jupyter-keymap",
      "ms-toolsai.jupyter-renderers",
      "ms-toolsai.vscode-jupyter-cell-tags",
      "ms-toolsai.vscode-jupyter-slideshow",
      "ms-toolsai.vscode-jupyter-powertoys",
      "ms-python.python"
    ],
    "terminal.integrated.sendKeybindingsToShell": true,
    "cursor.chat.terminalShowHoverHint": false,
    "cursor.terminal.usePreviewBox": true,
    "terminal.integrated.localEchoLatencyThreshold": 200
}
